{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/Public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/Public/js/$1"
    },
    {
      "src": "/(.*\\.(html|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/Public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 10
    }
  }
}